&i2c4 {
	status = "okay";
	clock-frequency = <400000>;

	hisense_nfc: nfc-nci@28{
		compatible = "nxp,nfc-nci";
		reg = <0x28>;
		nxp,firm-gpio  = <&ap_gpio 27 0>;
		nxp,irq-gpio   = <&ap_gpio 30 0>;
		nxp,ven-gpio   = <&ap_gpio 26 0>;
		status = "ok";
